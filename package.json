{
  "name": "adonis-api-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "Adonisjs start with graphql",
  "main": "server.js",
  "scripts": {
    "start:dev": "yarn pm2:watch && yarn pm2:logs",
    "setup": "yarn install",
    "startup:prod": "NODE_ENV=production yarn setup && yarn start:prod",
    "startup:dev": "NODE_ENV=development yarn setup && yarn start",
    "startup:test": "NODE_ENV=testing yarn setup && yarn test",
    "start:prod": "yarn pm2:production && yarn pm2:logs",
    "start": "nodemon --no-warnings server.js",
    "dev:monit": "pm2 monit graphql-server",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "test": "nodemon --no-warnings ace test",
    "pm2:watch": "pm2 start --watch",
    "pm2:production": "pm2 start --env production -i max",
    "pm2:prune": "pm2 delete graphql-server",
    "pm2:stop": "pm2 stop graphql-server",
    "pm2:logs": "pm2 logs -n graphql-server --raw",
    "pm2:flush": "pm2 flush",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "docker:dev": "docker-compose up",
    "docker:build": "docker-compose up"
  },
  "keywords": [
    "adonisjs",
    "adonis-app",
    "graphql",
    "apollo-graphql",
    "api graphql",
    "adonis mongodb",
    "graphql mondodb"
  ],
  "author": "Jairo Duarte",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/redis": "^2.0.7",
    "@adonisjs/validator": "^5.0.3",
    "@adonisjs/vow": "^1.0.17",
    "apollo-server": "^2.25.3",
    "apollo-server-adonis": "^1.4.0",
    "graphql": "^14.4.2",
    "graphql-playground-html": "^1.6.13",
    "graphql-tools": "^4.0.5",
    "lodash": "^4.17.14",
    "lucid-mongo": "^3.1.6",
    "nodemon": "^1.19.1",
    "pm2": "^3.5.1"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.12.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  },
  "autoload": {
    "App": "./app"
  }
}
